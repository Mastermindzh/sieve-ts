{
  "name": "sieve-ts",
  "version": "1.1.1",
  "license": "MIT",
  "author": "Rick van Lieshout",
  "main": "dist/index.js",
  "module": "dist/sieve-ts.esm.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://git.mastermindzh.tech/mastermindzh/sieve-ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test",
    "test-verbose": "dts test --silent=false --verbose"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/sieve-ts.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sieve-ts.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@tsconfig/recommended": "^1.0.2",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  }
}